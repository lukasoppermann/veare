  "scripts": {
    "start": "node_modules/.bin/ttab 'npm run supervisor' && node_modules/.bin/ttab 'gulp'",
    "stop": "forever stopall",
    "build:css": "node-sass --output-style compressed --source-map true --source-map-contents sass resources/css/app.scss > test.css",
      "css:clean": "rm -r public/css/*",
      "css:compile": "node-sass --output-style compressed resources/css/app.scss > public/css/app.css",
      "css:rev": "node node-file-rev.js --input='public/css/app.css' --manifest='public/manifest-rev.json'",
    "supervisor": "supervisor -w . --extensions node,js,hbs app.js",
    "forever": "forever start -v --watch --watchDirectory=/Users/lukasoppermann/code/veare app.js",
    "test": "standard 'app.js' 'resources/js/*.js' 'app/**/*.js' 'gulp-tasks/*.js' | snazzy",
    "travis": "npm run test",
    "removeCompiled": "branch=$(git branch | grep \\* | cut -d ' ' -f2) && if test \"$branch\" != \"master\" ; then git reset HEAD public/ ; fi"
  },
  "pre-commit": [
    "test",
    "removeCompiled"
  ]
