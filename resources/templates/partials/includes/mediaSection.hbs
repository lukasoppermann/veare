<page-section centered maxwidth="1200px" class="Section Section__media{{#each section.stylingOptions}} Section__media--{{this}}{{/each}} {{section.classes}}">
  <grid-container>
      {{#if (eq section.media.length 1)}}
        {{> (join "includes/singleMediaSection") section=section}}
      {{else}}
        {{> (join "includes/multiMediaSection") section=section}}
      {{/if}}
  </grid-container>
</page-section>
